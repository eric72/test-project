{
  "name": "projetpb",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy:env",
    "build:dev": "nest build && pm2 start pm2.yml && npm run copy:env",
    "build:prod": "nest build && pm2 start pm2.yml -n projetpb --env production && npm run copy:env",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start && environnement=prod",
    "start:dev": "nest start --watch && environnement=dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main && environnement=prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy:env": "cp '.env' './dist/.env'",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/database.config.ts",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:show": "npm run typeorm migration:show"
  },
